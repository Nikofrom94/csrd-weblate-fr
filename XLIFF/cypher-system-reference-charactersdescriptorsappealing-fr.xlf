      <trans-unit xml:space="preserve" id="dbf2e22646d1b5c7" weblate-flags="md-text, safe-html, strict-same">
        <source>Appealing #Descriptor #Core</source>
        <target></target>
        <context-group name="" purpose="location">
          <context context-type="sourcefile"></context>
